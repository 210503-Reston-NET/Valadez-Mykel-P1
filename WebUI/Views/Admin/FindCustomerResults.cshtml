
@model WebUI.Models.CustomerVM;
@{ ViewData["Title"] = "FindCustomerResults"; }



<h1>Customer Search Results</h1>

@if(Model == null){
    <h1 style="text-align: center; margin-top: 50px">No Customer Found</h1>
}

@if(Model != null ){
<table class="table">

    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>
        </tr>
    </thead>
    <tbody>
<tr>
    <td>
        @Html.DisplayFor(modelItem => Model.Name)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.Email)
    </td>
    <td>
</tr>
    </tbody>
</table>}

<button onclick="location.href='@Url.Action("Index", "Admin")'" class="btn-primary">
    Back
</button>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
